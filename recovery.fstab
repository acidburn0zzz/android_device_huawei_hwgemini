# mount point	fstype    device    	[device2]

/cache	ext4	/dev/block/mmcblk0p34 	rw,seclabel,relatime,data=ordered 0 0
/system	ext4	/dev/block/mmcblk0p38 	ro,seclabel,relatime,data=ordered 0 0
/data	ext4	/dev/block/mmcblk0p40 	rw,seclabel,nosuid,nodev,noatime,discard,noauto_da_alloc,data=ordered 0 0
/misc	emmc	/dev/block/mmcblk0p19
/boot	emmc	/dev/block/mmcblk0p27
/recovery emmc	/dev/block/mmcblk0p28
/sd-ext	vfat	/dev/block/mmcblk1p1

